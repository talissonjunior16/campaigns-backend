{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug NestJS",
        "type": "node",
        "request": "launch",
        "args": ["${workspaceFolder}/src/main.ts"],
        "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
        "sourceMaps": true,
        "cwd": "${workspaceFolder}",
        "protocol": "inspector",
        "console": "integratedTerminal",
        "autoAttachChildProcesses": true,
        "restart": true
      }
    ]
  }
  